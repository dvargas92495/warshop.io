    "esbuild": "^0.12.24",
    "eslint": "^7.32.0",
    "fuegojs": "^0.16.2",
    "prettier": "^2.3.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@dplusic/gamelift-nodejs-serversdk": "0.0.1",
